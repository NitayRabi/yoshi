{
  "name": "yoshi-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "version": "docusaurus docs:version",
    "publish-janet": "(git diff  --name-status origin/master | grep 'website/') && janet deploy --ci || echo 'No changes to 'website' directory detected.\nSkipping Janet PR build'"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.59",
    "@docusaurus/preset-classic": "2.0.0-alpha.59",
    "classnames": "2.2.6",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "janet": "^1.0.86"
  }
}
